build
coverage
dist
node_modules
pnpm-lock.yaml
.turbo

# Ignore files from version control
.git

# Ignore package manager files
package-lock.json
yarn.lock

# Ignore sensitive files
.env
.env.*

# Ignore logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*
.pnpm-debug.log*

# Ignore IDE/editor files
.idea
.vscode

# Ignore OS-specific files
.DS_Store
Thumbs.db
