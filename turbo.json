{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"globalDependencies": ["**/.env.*local"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "! .next/cache/**", "dist/**"]
		},
		"lint": {},
		"check-types": {},
		"test": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"dev": {
			"dependsOn": ["^build"],
			"cache": false,
			"persistent": true
		},
		"watch": {
			"cache": false,
			"persistent": true,
			"outputs": []
		}
	}
}
